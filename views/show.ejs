<%- include('partials/header') %>

<div class="thumbnail">
  <img src="<%=campground.image%>" alt="" />
  <div class="caption">
    <h2><%=campground.name%></h2>
    <p><%=campground.description%></p>
  </div>
  <% campground.comments.map(comment => { %>
  <p><%= comment.text %> -- <strong><%= comment.author %></strong></p>
  <% }) %>
</div>

<%-include('partials/footer') %>
